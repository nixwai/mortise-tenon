{
  "name": "mortise-tenon-design",
  "type": "module",
  "version": "0.4.0-beta.2",
  "description": "榫卯组件库",
  "author": "nixwai",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/nixwai/mortise-tenon"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*.mjs": {
      "types": "./types/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./types/*.d.ts",
        "./types/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": [
        "./types/*.d.ts",
        "./types/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "types/index.d.ts",
  "files": ["es", "global.d.ts", "lib", "types"],
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@vueuse/core": "^11.1.0",
    "lodash-es": "^4.17.21"
  }
}
